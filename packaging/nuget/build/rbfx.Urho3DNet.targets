<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <ItemGroup>
        <Reference Include="Urho3DNet">
            <HintPath>$(MSBuildThisFileDirectory)../lib/$(TargetFramework)/Urho3DNet.dll</HintPath>
            <Private>false</Private>
        </Reference>
        <Reference Include="Urho3DNet.Scripts">
            <HintPath>$(MSBuildThisFileDirectory)../lib/$(TargetFramework)/Urho3DNet.Scripts.dll</HintPath>
            <Private>false</Private>
        </Reference>
    </ItemGroup>
    <ItemGroup>
        <AndroidJavaSource Include="$(MSBuildThisFileDirectory)/../content/java/**/*.java">
            <Link>%(RecursiveDir)%(FileName)%(Extension)</Link>
        </AndroidJavaSource>
    </ItemGroup>
</Project>