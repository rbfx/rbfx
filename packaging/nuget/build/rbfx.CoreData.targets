<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <ItemGroup>
        <AndroidAsset Include="$(MSBuildThisFileDirectory)/../content/CoreData/**">
            <Link>%(RecursiveDir)%(FileName)%(Extension)</Link>
            <TargetPath>CoreData/%(RecursiveDir)%(FileName)%(Extension)</TargetPath>
        </AndroidAsset>
    </ItemGroup>
    <ItemGroup>
        <None Include="$(MSBuildThisFileDirectory)/../content/CoreData/**">
            <Link>CoreData/%(RecursiveDir)%(FileName)%(Extension)</Link>
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
            <TargetPath>CoreData/%(RecursiveDir)%(FileName)%(Extension)</TargetPath>
        </None>
    </ItemGroup>
</Project>
