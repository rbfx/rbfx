# This is a very basic version compatibility check
# It uses the SameMajorVersion compatibility mode by default

set(PACKAGE_VERSION "@CMAKE_PROJECT_VERSION@")

if(PACKAGE_FIND_VERSION_MAJOR EQUAL "@CMAKE_PROJECT_VERSION_MAJOR@")
    if(PACKAGE_FIND_VERSION_COUNT EQUAL 1)
        # Major version match is sufficient
        set(PACKAGE_VERSION_COMPATIBLE TRUE)
    elseif(PACKAGE_FIND_VERSION_MINOR LESS_EQUAL "@CMAKE_PROJECT_VERSION_MINOR@")
        # Major.minor version is compatible
        set(PACKAGE_VERSION_COMPATIBLE TRUE)
        
        if(PACKAGE_VERSION VERSION_EQUAL PACKAGE_FIND_VERSION)
            set(PACKAGE_VERSION_EXACT TRUE)
        endif()
    else()
        set(PACKAGE_VERSION_COMPATIBLE FALSE)
    endif()
else()
    set(PACKAGE_VERSION_COMPATIBLE FALSE)
endif()