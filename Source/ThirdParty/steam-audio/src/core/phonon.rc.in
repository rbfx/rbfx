1 VERSIONINFO
    FILEVERSION     @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
    PRODUCTVERSION  @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
    FILEFLAGSMASK   0x3f
    FILEFLAGS       0x0
    FILEOS          0x40004
    FILETYPE        0x2
    FILESUBTYPE     0x0
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName",        "Valve Corporation"
            VALUE "ProductName",        "Steam Audio"
            VALUE "ProductVersion",     "@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@"
            VALUE "FileDescription",    "Steam Audio Core"
            VALUE "FileVersion",        "@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@"
            VALUE "InternalName",       "Steam Audio Core"
            VALUE "OriginalFilename",   "phonon.dll"
            VALUE "LegalCopyright",     "https://github.com/ValveSoftware/steam-audio/blob/master/LICENSE.md"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 0x04b0
    END
END
