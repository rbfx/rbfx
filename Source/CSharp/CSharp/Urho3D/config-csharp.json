  "typemaps": [
    {
      "type": "Urho3D::StringHash",
      "ctype": "unsigned",
      "ptype": "uint",
      "cstype": "Urho3D.StringHash",
      "cpp_to_c": "{value}.Value()",
      "c_to_cpp": "Urho3D::StringHash({value})",
      "pinvoke_to_cs": "new StringHash({value})",
      "cs_to_pinvoke": "{value}.Hash",
      "is_value_type": true,
    },
    // TODO: Avoid string copying when possible.
    // String typemaps always return a copy of string which is freed by mono. Sometimes this is not necessary and c api
    // could return pointer pointing to string content held by the engine. However mono is required to not free such
    // string after conversion, which implies handling it manually. Also investigate how .net framework behaves in this
    // situation.
    {
      "type": "Urho3D::String",
      "ctype": "char const*",
      "ptype": "string",
      "cstype": "string",
      "cpp_to_c": "{value}.CString()",
      "c_to_cpp": "Urho3D::String({value})",
      "is_value_type": true,
      "supports_wchar": true,
    },
    {
      "type": "Urho3D::VariantMap",
      "ctype": "Urho3D::VariantMap*",
      "ptype": "IntPtr",
      "cstype": "Urho3D.VariantMap",
      "cpp_to_c": "CSharpObjConverter::ToCSharp<Urho3D::VariantMap>({value})",
      "c_to_cpp": "*{value}",
      "pinvoke_to_cs": "VariantMap.__FromPInvoke({value}, {owns})",
      "cs_to_pinvoke": "VariantMap.__ToPInvoke({value})",
    },
    {
      "type": "Urho3D::StringVector",
      "ctype": "Urho3D::StringVector*",
      "ptype": "IntPtr",
      "cstype": "Urho3D.StringVector",
      "cpp_to_c": "CSharpObjConverter::ToCSharp<Urho3D::StringVector>({value})",
      "c_to_cpp": "*{value}",
      "pinvoke_to_cs": "StringVector.__FromPInvoke({value}, {owns})",
      "cs_to_pinvoke": "StringVector.__ToPInvoke({value})"
    },
    {
      "type": "Vector<Urho3D::String>",
      "ctype": "Urho3D::StringVector*",
      "ptype": "IntPtr",
      "cstype": "Urho3D.StringVector",
      "cpp_to_c": "CSharpObjConverter::ToCSharp<Urho3D::StringVector>({value})",
      "c_to_cpp": "*{value}",
      "pinvoke_to_cs": "StringVector.__FromPInvoke({value}, {owns})",
      "cs_to_pinvoke": "StringVector.__ToPInvoke({value})"
    },
    {
      "type": "Urho3D::Vector2",
      "ctype": "Urho3D::PODVector2",
      "cstype": "Urho3D.Vector2",
      "ptype": "Urho3D.Vector2",
      "cpp_to_c": "CSharpPODConverter<Urho3D::Vector2>::ToCSharp({value})",
      "c_to_cpp": "CSharpPODConverter<Urho3D::Vector2>::FromCSharp({value})",
      "is_value_type": true,
